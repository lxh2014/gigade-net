<!DOCTYPE html>
<html>
<head>
    <style type="text/css">
        html:
        {
            overflow: hidden;
        }
        
   
    </style>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/Ext4.0/resources/css/ext-all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/ToolIcons.css?v=" + @Context.Application["Version"].ToString() + "")" rel="stylesheet" type="text/css" />
    @RenderSection("Css", false)
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Ext4.0/ext-all.js?v=1.0.1")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Ext4.0/locale/ext-lang-" + @Html.Raw(Request.Cookies["lang"] == null ? "zh-TW" : Request.Cookies["lang"].ToString()) + ".js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Locale/" + @Html.Raw(Request.Cookies["lang"] == null ? "zh-TW" : Request.Cookies["lang"].ToString()) + "/lang.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Common/ToolAuthority.js?v="+@Context.Application["Version"].ToString()+"")" type="text/javascript"></script>
    <script src="../../Scripts/jquery.uploadify-v2.1.0/swfobject.js" type="text/javascript"></script>
    <link href="../../Scripts/jquery.uploadify-v2.1.0/uploadify.css" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/icon.css")" type="text/css" rel="stylesheet" />
    <script src="../../Scripts/jquery.uploadify-v2.1.0/jquery.uploadify.v2.1.0.js" type="text/javascript"></script>
    @RenderSection("Js", false)
</head>
<body>
    @RenderBody()
</body>
</html>
