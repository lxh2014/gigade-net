@using BLL.gigade.Common
@using BLL.gigade.Model
@{
    string productId = ViewBag.ProductId;
    string oldProductId = ViewBag.OldProductId;
    string hfAuth = ViewBag.hfAuth;
    string hfAspSessID = ViewBag.hfAspSessID;
}
<script src="../../Locale/@Html.Raw(Request.Cookies["lang"] == null ? "zh-TW" : Request.Cookies["lang"].ToString())/Product/ProductLang.js?v=@Context.Application["Version"].ToString()" type="text/javascript"></script>
<script src="../../Locale/@Html.Raw(Request.Cookies["lang"] == null ? "zh-TW" : Request.Cookies["lang"].ToString())/ProductTier/ExpectTierLang.js?v=@Context.Application["Version"].ToString()" type="text/javascript"></script>
<script src="../../Scripts/Views/ProductCombo/ProductComboSave.js?v=@Context.Application["Version"].ToString()" type="text/javascript"></script>
<script src="../../Scripts/Ext4.0/src/form/field/DateTimePicker.js" type="text/javascript"></script>
<script src="../../Scripts/Ext4.0/src/form/field/DateTimeField.js" type="text/javascript"></script>
<script src="../../Scripts/Views/ProductTier/ExpectTier.js?v=@Context.Application["Version"].ToString()" type="text/javascript"></script>


@Html.Hidden("ProductId", productId)
@Html.Hidden("OldProductId", oldProductId)
@Html.Hidden("ProductMode")
@Html.Hidden("hfAuth", hfAuth)
@Html.Hidden("hfAspSessID", hfAspSessID)
@Html.Hidden("batchno", CommonFunction.GetPHPTime() + "_" + (Session["caller"] as Caller).user_id + "_" + productId)
@Html.Partial("Loading", 2000)
