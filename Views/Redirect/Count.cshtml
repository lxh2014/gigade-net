@{
    ViewBag.Title = "Count";
}
    <style>
        .contain-title {
            background: #FFF;
            color: #FF8402;
            font-size: 18px;
            font-weight: bold;
            border: 1px none #595959;
            border-bottom-style: dashed;
            padding-bottom: 10px;
            margin-bottom: 10px;
            border-bottom: 0 none;
        }

            .contain-title p {
                margin: 0;
                float: right;
            }

        body {
            height: 100%;
            margin: 0;
            padding: 0;
            text-decoration: none;
            color: #666;
            font-size: 14px;
            font-family: Arial, Helvetica, sans-serif;
        }
    </style>

@section Js{
    <script type="text/javascript" src="../../Locale/@Html.Raw(Request.Cookies["lang"] == null ? "zh-TW" : Request.Cookies["lang"].ToString())/Redirect/RedirectLang.js?v=@Context.Application["Version"].ToString()"></script>
    <script src="../../Scripts/Ext4.0/src/form/field/DateTimePicker.js" type="text/javascript"></script>
    <script src="../../Scripts/Ext4.0/src/form/field/DateTimeField.js" type="text/javascript"></script>
    <script src="../../Scripts/Views/Redirect/Count.js?v=@Context.Application["Version"].ToString()" type="text/javascript"></script>
    <input type="hidden" id="group_id" value="@ViewBag.group_id"/>
    <input type="hidden" id="group_name" value="@ViewBag.group_name"/>

    }